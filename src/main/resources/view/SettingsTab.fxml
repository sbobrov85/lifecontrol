<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.sbobrov85.lifecontrol.controllers.SettingsTabController">
   <children>
      <TabPane prefHeight="400.0" prefWidth="606.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <tabs>
          <Tab closable="false" text="%Categories">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TreeView fx:id="categoriesTree" layoutY="59.0" prefHeight="295.0" prefWidth="572.0" AnchorPane.bottomAnchor="13.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="59.0" />
                        <ToolBar layoutX="14.0" layoutY="14.0">
                           <items>
                              <Button fx:id="addButton" mnemonicParsing="false" onAction="#addCategory" text="%Add" />
                              <Button fx:id="editButton" mnemonicParsing="false" onAction="#editCategory" text="%Edit" />
                              <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteCategory" text="%Delete" />
                           </items>
                        </ToolBar>
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab closable="false" text="%Options">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
